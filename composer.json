{
    "name": "uginroot/async-symfony-process",
    "description": "Execute pool symfony process",
    "type": "library",
    "require": {
        "php": ">=7.4",
        "symfony/process": "^3.4|^4.4|^5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "8.0.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Soroka Anton",
            "email": "uginroot@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Uginroot\\AsyncSymfonyProcess\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Uginroot\\AsyncSymfonyProcess\\Test\\": "tests"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
